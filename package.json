{
  "name": "20-react-portfolio",
  "version": "1.0.0",
  "description": "My Portfolio",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
    "client": "cd client && yarn start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaaniBravo/React-Portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.16.3",
    "if-env": "^1.0.4"
  },
  "bugs": {
    "url": "https://github.com/TaaniBravo/React-Portfolio/issues"
  },
  "homepage": "https://taani.dev"
}
